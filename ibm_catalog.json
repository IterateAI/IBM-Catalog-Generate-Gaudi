            "features": [
			  {
                "title": "Agentic AI Cards",
                "description": "Pre-built and customizable Agentic AI Cards designed for vertical-specific tasks — from analyzing earnings calls to generating healthcare procedure growth reports, to drafting RFPs. Cards work across document and database searches for maximum flexibility."
              },
			  {
                "title": "Document Search",
                "description": "Bring your documents into Generate and instantly run advanced Retrieval Augmented Generation (RAG) queries. Summarize lengthy reports, extract specific answers, or generate action items — all within seconds."
              },
              {
                "title": "Database Search",
                "description": "Query your structured data using plain English. Generate intelligently interprets your questions and retrieves actionable insights directly from your databases."
              },
              {
                "title": "Service Pilot",
                "description": "Connect your email inbox and empower your team with context-aware summaries and intelligent reply suggestions."
              }
